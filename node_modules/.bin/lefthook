#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lbernat/spaceship/node_modules/.pnpm/@evilmartians+lefthook@2.1.0/node_modules/@evilmartians/lefthook/bin/node_modules:/home/lbernat/spaceship/node_modules/.pnpm/@evilmartians+lefthook@2.1.0/node_modules/@evilmartians/lefthook/node_modules:/home/lbernat/spaceship/node_modules/.pnpm/@evilmartians+lefthook@2.1.0/node_modules/@evilmartians/node_modules:/home/lbernat/spaceship/node_modules/.pnpm/@evilmartians+lefthook@2.1.0/node_modules:/home/lbernat/spaceship/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lbernat/spaceship/node_modules/.pnpm/@evilmartians+lefthook@2.1.0/node_modules/@evilmartians/lefthook/bin/node_modules:/home/lbernat/spaceship/node_modules/.pnpm/@evilmartians+lefthook@2.1.0/node_modules/@evilmartians/lefthook/node_modules:/home/lbernat/spaceship/node_modules/.pnpm/@evilmartians+lefthook@2.1.0/node_modules/@evilmartians/node_modules:/home/lbernat/spaceship/node_modules/.pnpm/@evilmartians+lefthook@2.1.0/node_modules:/home/lbernat/spaceship/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@evilmartians/lefthook/bin/index.js" "$@"
else
  exec node  "$basedir/../@evilmartians/lefthook/bin/index.js" "$@"
fi
